{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "release-type": "go",
      "exclude-paths": [
        "docs", "scripts"
      ]
    }
  },
  "changelog-sections": [
		{
			"type": "feat",
			"section": "Features"
		},
		{
			"type": "fix",
			"section": "Bug Fixes"
		},
		{
			"type": "deps",
			"section": "Dependencies"
		}
  ]
}
